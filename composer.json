{
    "name": "prograp/composer-dependency-ignore",
    "type": "composer-plugin",
    "version": "1.0.0",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Pro\\Composer\\DependencyIgnore\\": "src/"
        }
    },

    "require": {
        "composer-plugin-api": "^2.0"
    },

    "require-dev": {
        "composer/composer": "^2.0",
        "phpstan/phpstan": "^0.12.99",
        "phpmd/phpmd": "^2.10"
    },

    "extra": {
        "class": "Pro\\Composer\\SwDependencySolver\\Plugin"
    }
}
